${FUNCTION_SIGNATURE} {
	${PREPARATION}

	stmt, err := db.Prepare(`${SQL_STATEMENT}`)
	if err != nil {
		log.Fatal(err)
	}

	res, err := stmt.Exec(${ARGUMENTS})
	if err != nil {
		log.Fatal(err)
	}

	lastId, err := res.LastInsertId()
	if err != nil {
		log.Fatal(err)
	}

	rowCnt, err := res.RowsAffected()
	if err != nil {
		log.Fatal(err)
	}

	log.Printf("ID = %d, affected = %d\n", lastId, rowCnt)
}
